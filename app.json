{
  "expo": {
    "name": "Schat",
    "slug": "Schat",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/log.png",
      "resizeMode": "contain",
      "backgroundColor": "#F97100"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/log.png",
        "backgroundColor": "#F97100"
      },
      "package": "com.yusufwebdev.Schat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
